//.カフェ
document.getElementById("cafe").onclick = function(){
      if (map && addMarker) {
        map.removeLayer(addMarker);
        addMarker = null;
    }
          var popup = L.popup();
 addMarker = L.marker([34.78124404234647, 138.01317869983708]).addTo(map).on('click', function (e) {
    popup
      .setLatLng(e.latlng)
      .setContent("aaa".fontcolor(""))
      .openOn(map);
      });
 }

